# 1 "pgcd.c"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "/usr/include/stdc-predef.h" 1 3 4
# 1 "<command-line>" 2
# 1 "pgcd.c"


int
pgcd(int a, int b){
        int t;

        while(a != 0){
                if (a < b){
                        t = b;
                        b = a;
                        a = t;
                }
                a -= b;
        }
        return b;
}
